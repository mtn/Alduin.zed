{
  "$schema": "https://zed.dev/schema/themes/v0.2.0.json",
  "author": "mtn (originally Alessandro Yorba)",
  "name": "Alduin",
  "themes": [
    {
      "appearance": "dark",
      "name": "Alduin",
      "style": {
        "accents": ["#dfaf87", "#af875f", "#af5f5f", "#87afaf"],
        "background": "#1c1c1c",
        "background.appearance": "opaque",

        "text": "#dfdfaf",
        "text.muted": "#878787",
        "text.disabled": "#6c6c6c",
        "text.accent": "#dfaf87",
        "text.placeholder": "#626262",

        "border": "#444444",
        "border.variant": "#303030",

        "surface.background": "#121212",
        "elevated_surface.background": "#262626",

        "status_bar.background": "#121212",
        "toolbar.background": "#121212",
        "title_bar.background": "#121212",
        "title_bar.inactive_background": "#121212",

        "tab_bar.background": "#121212",
        "tab.active_background": "#121212",
        "tab.inactive_background": "#121212",

        "panel.background": "#121212",

        "editor.background": "#1c1c1c",
        "editor.foreground": "#dfdfaf",
        "editor.gutter.background": "#121212",
        "editor.line_number": "#4e4e4e",
        "editor.active_line_number": "#9e9e9e",
        "editor.highlighted_line.background": "#262626",
        "editor.active_line.background": "#262626",
        "editor.wrap_guide": "#303030",
        "editor.active_wrap_guide": "#444444",
        "editor.indent_guide": "#303030",
        "editor.indent_guide_active": "#444444",
        "editor.invisible": "#303030",

        "search.match_background": "#4a4020",

        "players": [
          {
            "selection": "#af875f80"
          }
        ],

        "error": "#af5f5f",
        "warning": "#dfaf87",
        "warning.background": "#332f1a",
        "warning.border": "#dfaf87",
        "info": "#87afaf",
        "success": "#87875f",

        "created": "#87875f",
        "modified": "#dfaf87",
        "deleted": "#af5f5f",
        "renamed": "#87afaf",

        "terminal.background": "#1c1c1c",
        "terminal.foreground": "#dfdfaf",
        "terminal.dim_foreground": "#878787",
        "terminal.bright_foreground": "#dfdfaf",
        "terminal.ansi.background": "#1c1c1c",
        "terminal.ansi.black": "#1c1c1c",
        "terminal.ansi.red": "#af5f5f",
        "terminal.ansi.green": "#87875f",
        "terminal.ansi.yellow": "#dfaf87",
        "terminal.ansi.blue": "#878787",
        "terminal.ansi.magenta": "#875f5f",
        "terminal.ansi.cyan": "#87afaf",
        "terminal.ansi.white": "#dfdfaf",
        "terminal.ansi.bright_black": "#878787",
        "terminal.ansi.bright_red": "#af5f5f",
        "terminal.ansi.bright_green": "#87875f",
        "terminal.ansi.bright_yellow": "#dfaf87",
        "terminal.ansi.bright_blue": "#878787",
        "terminal.ansi.bright_magenta": "#875f5f",
        "terminal.ansi.bright_cyan": "#87afaf",
        "terminal.ansi.bright_white": "#dfdfaf",

        "syntax": {
          "comment": { "color": "#87875f" },
          "string": { "color": "#ffdf87", "background_color": "#262626" },
          "number": { "color": "#af5f00" },
          "constant": { "color": "#af8787" },
          "boolean": { "color": "#af5f00" },
          "float": { "color": "#af5f00" },
          "function": { "color": "#875f5f" },
          "keyword": { "color": "#878787" },
          "operator": { "color": "#878787" },
          "type": { "color": "#af875f" },
          "variable": { "color": "#87afaf" },
          "property": { "color": "#87afaf" },
          "attribute": { "color": "#875f5f" },
          "namespace": { "color": "#af875f" },
          "tag": { "color": "#dfaf87" },
          "punctuation": { "color": "#878787" },
          "delimiter": { "color": "#878787" },
          "builtin": { "color": "#dfaf87" },
          "escape": { "color": "#af5f5f" },
          "regexp": { "color": "#af8787" },
          "title": { "color": "#dfaf87" }
        }
      }
    }
  ]
}
